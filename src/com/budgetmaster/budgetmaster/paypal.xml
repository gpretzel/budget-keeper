<?xml version="1.0" encoding="UTF-8"?>
<config>
  <parser class="com.budgetmaster.budgetmaster.PayPalStatementCsv" id="PayPal statement">
    <path-matcher>glob:**/*.csv</path-matcher>
  </parser>
  
  <pass>
    <action>
      <matcher>
        <category>\bPayment$</category>
      </matcher>
      <negation/>
      <tag>paypal</tag>
    </action>

    <action><discard/></action>
  </pass>

</config>
